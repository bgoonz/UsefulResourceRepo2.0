{
  "name": "gatsby-source-netlify",
  "version": "1.0.4",
  "main": "index.js",
  "repository": "git@github.com:LekoArts/gatsby-source-netlify.git",
  "author": "LekoArts <lekoarts@gmail.com>",
  "license": "MIT",
  "keywords": [
    "netlify",
    "gatsby-plugin",
    "gatsby-source"
  ],
  "peerDependencies": {
    "gatsby": ">=2.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write README.md"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "netlify": "^2.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-preset-gatsby-package": "^0.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  }
}
