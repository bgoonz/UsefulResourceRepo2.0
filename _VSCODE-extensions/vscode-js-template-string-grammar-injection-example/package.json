{
  "name": "javascript-template-string-grammar-injection-example",
  "displayName": "JavaScript template string grammar injection example",
  "description": "Demonstrates how an extension can inject syntax highlighting for a specific language into a JavaScript template string",
  "version": "0.0.1",
  "publisher": "bierner",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mjbvz/vscode-js-template-string-grammar-injection-example.git"
  },
  "bugs": {
    "url": "https://github.com/mjbvz/vscode-js-template-string-grammar-injection-example/issues"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "meta.embedded.html.string.template",
        "path": "./syntaxes/injection.json",
        "injectTo": [
          "source.js",
          "source.jsx",
          "source.ts",
          "source.tsx"
        ]
      }
    ]
  },
  "dependencies": {}
}
