<configuration>
  <system.runtime.remoting>
    <application>

     <channels> 
      <channel ref="http">
	<clientProviders>
	   <formatter ref="soap" />
	   <provider type="CompressionSink.CompressionClientSinkProvider, CompressionSink" />
	</clientProviders>
      </channel>
      </channels>

      <client>
        <wellknown type="Service.SomeSAO, Service"  
                   url="http://localhost:5556/SomeSAO.soap" />

      </client>

    </application>
  </system.runtime.remoting>
</configuration>
